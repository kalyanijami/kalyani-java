import java.io.*;
import java.util.*;
class Circle{
    double r;
    double area(){
        return(22/7.0)*r*r;
    }
}
public class Main{
    public static void main(String args[]){
        Circle c1=new Circle();
        c.r=5.5;
        System.out.println("Area of Circle with radius "+c.r+" is: "+c.area());
    }
}
